<ul class="users-home">
  <% @all_users.each do |user| %>
  <li>
    <img src="<%= user.photo %>" alt="User Image">
    <a href="/users/<%= user.id %>" class="home-aside">
      <h2><%= user.name %></h2>
      <p>Number of Posts: <strong><%= user.posts_counter %></strong></p>
    </a>
  </li>
  <% end %>
</ul>
